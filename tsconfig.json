{
  "compilerOptions": {
    "target": "ESNext",  
    "module": "ESNext", 
    "outDir": "./dist",
    "rootDir": "./src", 
    "esModuleInterop": true,
    "strict": true, 
    "moduleResolution": "node",  
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["./dist"]
}